<div class="container">
  <div class="row">
    <div class="col s12">

    <h1>New Art Page</h1>

 <%= form_for @art, html: { multipart: true } do |f| %>
  <div>
    <h6>Add Your Own Art Image!</h6>
    <%= f.file_field :image %>
  </div>

  <div class="new-post-button">
  <%= f.submit "Create Post" %>
</div>


 <% end %>
    </div> <!-- col s12 -->


  </div> <!-- row -->
</div> <!-- container -->
