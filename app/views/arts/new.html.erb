<div class="container">
  <div class="row">
    <div class="col s12">
    <br>
    <h3>Add a photo for the Tag Analyzer</h3>
<br>
 <%= form_for @art do |f| %>
    <div class="image-uploadcare-button">

    <%= f.hidden_field :image, role: "uploadcare-uploader" %>
    <br>
    <h6 class="new-post-image-text-for-button">Add Your Own Art Image!</h6>
    </div>

<button class="btn waves-effect waves-light green">
  <%= f.submit "Create Post" %>
</button>


 <% end %>
    </div> <!-- col s12 -->


  </div> <!-- row -->
</div> <!-- container -->
