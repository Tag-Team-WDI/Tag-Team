<div class="container">
  <div class="row">
    <div class="col s12">

    <h1>New Art Page</h1>

 <%= form_for @art do |f| %>
    <div class="image-uploadcare-button">
    <h6 class="new-post-image-text-for-button">Add Your Own Art Image!</h6>
    <%= f.hidden_field :image, role: "uploadcare-uploader" %>
    </div>

  <div class="new-post-button">
  <%= f.submit "Create Post" %>
</div>


 <% end %>
    </div> <!-- col s12 -->


  </div> <!-- row -->
</div> <!-- container -->
