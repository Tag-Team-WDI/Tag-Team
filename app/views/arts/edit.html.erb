<div class="container">
  <div class="row">
    <div class="col s3"></div>
    <div class="col s6">
      <%= form_for @art, url: edit_art_path(@art), method: :patch  do |f| %>
      <div>
        <%= f.hidden_field :image, role: "uploadcare-uploader" %>
      </div>
      <%= f.submit "Edit Tag" %>
      <% end %>
    </div>
    <div class="col s3"></div>
  </div>
</div>
